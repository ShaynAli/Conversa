function T = cnv_makeLabelFromAvi(name);
% T = cnv_makeLabelFromAvi(name);
if (nargin<2); 
    rootDir = pwd; 
end; 
trackingFile=fullfile(rootDir,'RawData',dataID,sprintf('%s.txt',dataID)); 
